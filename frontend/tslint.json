{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended",
        "tslint-config-airbnb"
    ],
    "jsRules": {},
    "rules": {
        "indent": [
            true, 
            "spaces", 
            4
        ],
        "ter-indent": [
            true,
            4
        ],
        "import-name": false,
        "no-console": false,
        "ordered-imports": false,
        "no-increment-decrement": false,
        "max-line-length": false,
        "no-namespace": false,
        "max-classes-per-file": false
    },
    "rulesDirectory": []
}
